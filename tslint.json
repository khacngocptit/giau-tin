{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "curly": true,
      "trailing-comma": [
        true,
        {
          "multiline": "always",
          "singleline": "never"
        }
      ],
      "quotemark": [
        true,
        "double"
      ],
      "semicolon": [true, "always", "ignore-bound-class-methods"],
      "member-access": [
        false
      ],
      "ordered-imports": [
        false
      ],
      "object-literal-key-quotes": [
        true,
        "as-needed"
      ],
      "no-shadowed-variable": [
        true,
        {
          "function": true
        }
      ],
      "indent": [true, "spaces", 4],
      "whitespace": [true, "check-branch", "check-operator", "check-typecast"],
      "no-consecutive-blank-lines": true,
      "member-ordering": [
        false
      ],
      "interface-name": [
        false
      ],
      "prefer-for-of": false,
      "arrow-parens": false,
      "no-console": false,
      "object-literal-sort-keys": false,
      "max-classes-per-file": false
    },
    "rulesDirectory": []
  }
  